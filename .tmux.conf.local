tmux_conf_battery_vbar_palette="gradient"
tmux_conf_battery_status_charging="üîå"
tmux_conf_battery_status_discharging="üîã"

tmux_conf_theme_clock_colour="$tmux_conf_theme_colour_4"
tmux_conf_theme_clock_style="24"

tmux_conf_copy_to_os_clipboard=true
tmux_conf_urlscan_options="--compact --dedupe"

set -g history-limit 20000
set -gq allow-passthrough on
set -g mouse on
set -g status-keys vi
set -g mode-keys vi

bind C-f run-shell "tmux neww tmux-sessionizer"

bind C-v split-window -h
bind C-s split-window -v
bind C-d split-window -vf -l10
bind L switch-client -l

bind h select-pane -L
bind j select-pane -D
bind k select-pane -U
bind l select-pane -R

tmux_conf_update_plugins_on_launch=false
tmux_conf_update_plugins_on_reload=true
tmux_conf_uninstall_plugins_on_reload=true

set -g @plugin 'tmux-plugins/tmux-cpu'
